{
  "resources": {
    "weatherapi": {
      "type": "project.v0",
      "path": "../eCommerce.NET.MinimalApi/eCommerce.NET.MinimalApi.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "angular": {
      "type": "dockerfile.v0",
      "path": "../eCommerce.NET.Angular/Dockerfile",
      "context": "../eCommerce.NET.Angular",
      "env": {
        "NODE_ENV": "production",
        "services__weatherapi__http__0": "{weatherapi.bindings.http.url}",
        "services__weatherapi__https__0": "{weatherapi.bindings.https.url}",
        "PORT": "{angular.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8000,
          "external": true
        }
      }
    },
    "react": {
      "type": "dockerfile.v0",
      "path": "../eCommerce.NET.React/Dockerfile",
      "context": "../eCommerce.NET.React",
      "env": {
        "NODE_ENV": "production",
        "REACT_APP_WEATHER_API_HTTP": "{weatherapi.bindings.http.url}",
        "REACT_APP_WEATHER_API_HTTPS": "{weatherapi.bindings.https.url}",
        "PORT": "{react.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8001,
          "external": true
        }
      }
    },
    "vue": {
      "type": "dockerfile.v0",
      "path": "../eCommerce.NET.Vue/Dockerfile",
      "context": "../eCommerce.NET.Vue",
      "env": {
        "NODE_ENV": "production",
        "VITE_WEATHER_API_HTTP": "{weatherapi.bindings.http.url}",
        "VITE_WEATHER_API_HTTPS": "{weatherapi.bindings.https.url}",
        "PORT": "{vue.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8002,
          "external": true
        }
      }
    }
  }
}